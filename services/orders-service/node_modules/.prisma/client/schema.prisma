generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Order {
  id        String   @id @default(cuid())
  userId    String
  total     Float
  status    String   @default("created")
  createdAt DateTime @default(now())
  items     String
}
